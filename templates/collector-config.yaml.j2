receivers:
  otlp:
    protocols:
      grpc:
      http:

exporters:
  azuremonitor:
    instrumentation_key: {{ instrumentation_key }}
    spaneventsenabled: true
processors:
  batch:

service:
  pipelines:
    metrics:
      receivers: [otlp]
      exporters: [azuremonitor]
    traces:
      receivers: [otlp]
      exporters: [azuremonitor]
    logs:
      receivers: [otlp]
      exporters: [azuremonitor]
  telemetry:
    logs:
      level: debug